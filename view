import flet as ft

b=[]
new_task=[]



def main(page: ft.Page):
    sistem = ft.TextField(label="Введите сюда систему:", multiline = True, min_lines = 1, max_lines = 3)
    page.add(ft.Row([sistem, ft.ElevatedButton("Enter")]))

ft.app(target=main)
